{
  "name": "taste",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "node-sass-chokidar": "^0.0.3",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "build-css": "node-sass-chokidar --include-path src/css -o src/css",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path src/css -o src/css --watch --recursive"
  }
}
