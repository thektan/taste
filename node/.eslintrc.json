{
  "extends": ["airbnb-base"],
  "env": {
    "browser": true,
    "node" : true,
    "es6": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "class-methods-use-this": ["error", {
        "exceptMethods": [
          "render"
        ]
      }
    ]
  }
}
