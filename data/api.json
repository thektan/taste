[
  {
    "description": "Creates a new place",
    "path": "/places/*",
    "method": "POST",
    "data": true,
    "auth": {
      "validator": "$auth != null"
    },
    "parameters": {
      "name": {
        "type": "string",
        "required": true
      }
    }
  },
  {
    "description": "Gets information about a place",
    "path": "/places/:id",
    "method": "GET",
    "data": true
  }
]
